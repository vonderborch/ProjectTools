<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="ProjectTools.App.Views.Pages.PrepareTemplate">

    <UniformGrid Columns="4" Rows="1">
        <ScrollViewer Name="ViewerPreprocessConfig" />
        <ScrollViewer Name="ViewerTemplateConfig" IsEnabled="False" />
        <ScrollViewer Name="ViewerSlugConfig" IsEnabled="False" />
        <ScrollViewer Name="ViewerTemplateGeneration" IsEnabled="False" />
    </UniformGrid>
</UserControl>