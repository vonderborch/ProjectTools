<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="ProjectTools.App.Views.Pages.PrepareTemplatePage">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="9*" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <UniformGrid Grid.Row="0" Columns="3" Rows="1">
            <ScrollViewer Name="ViewerPreprocessConfig" />
            <ScrollViewer Name="ViewerTemplateConfig" />
            <ScrollViewer Name="ViewerSlugConfig" />
        </UniformGrid>
        <SelectableTextBlock IsEnabled="False" Grid.Row="1" Name="SelectableTextBlockLog" />
    </Grid>
</UserControl>